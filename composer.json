{
    "name": "zhamao/framework",
    "description": "High performance QQ robot and web server development framework",
    "minimum-stability": "stable",
    "license": "Apache-2.0",
    "version": "2.0.0-a1",
    "authors": [
        {
            "name": "whale",
            "email": "crazysnowcc@gmail.com"
        },
        {
            "name": "swift",
            "email": "hugo_swift@yahoo.com"
        }
    ],
    "prefer-stable": true,
    "bin": [
        "bin/start"
    ],
    "require": {
        "php": ">=7.2",
        "swoole/ide-helper": "@dev",
        "ext-mbstring": "*",
        "swlib/saber": "^1.0",
        "doctrine/annotations": "~1.10",
        "ext-json": "*",
        "ext-posix": "*",
        "ext-ctype": "*",
        "ext-pdo": "*",
        "psy/psysh": "@stable",
        "symfony/console": "^5.1",
        "symfony/polyfill-ctype": "^1.18",
        "zhamao/connection-manager": "^1.0",
        "zhamao/console": "^1.0",
        "zhamao/config": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Custom\\": "src/Custom",
            "ZM\\": "src/ZM",
            "Module\\": "src/Module"
        },
        "files": [
            "src/ZM/global_functions.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    }
}
